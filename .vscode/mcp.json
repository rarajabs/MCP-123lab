        "time": {
            "command": "uvx",
            "args": [
                "mcp-server-time"
            ],
            "type": "stdio"
        },
        "github": {
            "type": "http",
            "url": "https://api.githubcopilot.com/mcp/",
            "headers": {
                "Authorization": "Bearer ${input:github_mcp_pat}"
            }
        }
    },
    "inputs": [
        {
            "type": "promptString",
            "id": "github_mcp_pat",
            "description": "GitHub Personal Access Token",
            "password": true
        }
    ]
}
